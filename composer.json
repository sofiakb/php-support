{
  "name"             : "sofiakb/php-support",
  "description"      : "A library for support",
  "type"             : "library",
  "license"          : "MIT",
  "version"          : "0.0.9-beta3",
  "require"          : {
    "php"           : "^7.2",
    "nesbot/carbon" : "^2.45",
    "symfony/dotenv": "^5.2"
  },
  "require-dev"      : {
    "phpunit/phpunit": "^9.5"
  },
  "autoload"         : {
    "psr-4"   : {
      "Sofiakb\\Support\\": "src/"
    },
    "classmap": [
      "tests/"
    ]
  },
  "time"             : "2021-03-10",
  "authors"          : [
    {
      "name"    : "Sofiane Akbly",
      "email"   : "contact.sofiakb@gmail.com",
      "homepage": "https://sofianeakbly.fr",
      "role"    : "Developer"
    }
  ],
  "support"          : {
    "email": "contact.sofiakb@gmail.com"
  },
  "minimum-stability": "stable",
  "prefer-stable"    : true,
  "config"           : {
    "preferred-install"  : "dist",
    "sort-packages"      : true,
    "optimize-autoloader": true
  }
}