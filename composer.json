{
    "name": "ssf/php-support",
    "description": "A library for support",
    "type": "library",
    "license": "MIT",
    "version": "0.0.7",
    "require": {
        "php": "^7.2",
        "symfony/dotenv": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ssf\\Support\\": "src/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "time": "2021-03-03",
    "authors": [
        {
            "name": "Sofiane Akbly",
            "email": "sofiane.akbly@gmail.com",
            "homepage": "https://sofianeakbly.fr",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "sofiane.akbly@gmail.com"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}